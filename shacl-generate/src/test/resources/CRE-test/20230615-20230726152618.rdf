   <eli:WorkSubdivision rdf:about="https://data.europarl.europa.eu/eli/dl/doc/CRE-9-2023-06-12-SPEECH-4-026-0000">
      <dcterms:identifier>CRE-9-2023-06-12-SPEECH-4-026-0000</dcterms:identifier>
      <epvoc:numbering>4-026-0000</epvoc:numbering>
      <skos:notation rdf:datatype="https://data.europarl.europa.eu/def/epvoc#speechId">4-026-0000</skos:notation>
