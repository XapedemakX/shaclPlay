      <epvoc:numbering>1-267-0000</epvoc:numbering>
      <skos:notation rdf:datatype="https://data.europarl.europa.eu/def/epvoc#speechId">1-267-0000</skos:notation>
      <epvoc:originalLanguage rdf:resource="http://publications.europa.eu/resource/authority/language/EN"/>
      <eli:is_realized_by>
         <eli:Expression rdf:about="https://data.europarl.europa.eu/eli/dl/doc/CRE-9-2023-05-31-SPEECH-1-267-0000/en">
            <eli:language rdf:resource="http://publications.europa.eu/resource/authority/language/EN"/>
            <epvoc:expressionContent rdf:parseType="Literal">
               <p>
                  <em>Member of the Commission</em>
                  <strong>.</strong>  – Mr President, I welcome the debate on this important aspect of our responsibilities as employers and the opportunity to listen to different viewpoints and perspectives. </p>
               <p>The changes we have seen in the way we work post-COVID have brought with it new challenges. But the phenomenon of harassment has shown that it is able to adapt also to the digital environment. This will require employers to adapt and modernise our respective toolboxes to tackle the phenomenon of harassment. </p>
            </epvoc:expressionContent>
         </eli:Expression>
      </eli:is_realized_by>
      <eli:is_part_of rdf:resource="https://data.europarl.europa.eu/eli/dl/doc/CRE-9-2023-05-31-ITM-18"/>
