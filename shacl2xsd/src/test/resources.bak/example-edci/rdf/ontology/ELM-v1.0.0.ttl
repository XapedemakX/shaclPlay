elm:EuropassCredential 
   a rdfs:Class;
   a owl:Class;
   dc:identifier "elm:EuropassCredential";
   rdfs:comment "A set of claims made by an issuer in Europe, using the Europass Standards. A Europass credential is a set of one or more claims which may be used to demonstrate that the owner has certain skills or has achieved certain learning outcomes through formal, non-formal or informal learning."@en;
   rdfs:label "Europass Credential"@en;
   rdfs:isDefinedBy <https://data.europa.eu/snb/model>;
   rdfs:subClassOf vc:VerifiableCredential;
.
elm:EuropassPresentation
  a rdfs:Class;
  a owl:Class;
  dc:identifier "elm:EuropassPresentation";
  rdfs:comment "A verifiable presentation of a set of Europass credentials."@en;
  rdfs:label "Europass Presentation"@en;
  rdfs:isDefinedBy <https://data.europa.eu/snb/model>;
  rdfs:subClassOf vp:VerifiablePresentation;
.

elm:GradingScheme
  a rdfs:Class;
  a owl:Class;
  dc:identifier "elm:GradingScheme";
  rdfs:comment "A set of criteria that measures varying levels of achievement."@en;
  rdfs:label "Grading Scheme"@en;
  rdfs:isDefinedBy <https://data.europa.eu/snb/model>;
.

elm:Identifier
  a rdfs:Class;
  a owl:Class;
  dc:identifier "elm:Identifier";
  rdfs:comment "An Identifier."@en;
