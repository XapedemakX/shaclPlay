<rdf:Description rdf:about="http://data.europa.eu/snb/learning-setting/label_438c153d52f3">
	<created xmlns="http://purl.org/dc/terms/" rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2021-01-16</created>
	<rdf:type rdf:resource="http://www.w3.org/2008/05/skos-xl#Label"/>
