</rdf:Description>

<rdf:Description rdf:about="http://data.europa.eu/snb/learning-setting/label_0af8e82ad24a">
	<created xmlns="http://purl.org/dc/terms/" rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2021-01-15</created>
	<rdf:type rdf:resource="http://www.w3.org/2008/05/skos-xl#Label"/>
	<versionInfo xmlns="http://www.w3.org/2002/07/owl#">v.2.</versionInfo>
