    <xs:complexType name="EuropassPresentationType">
        <xs:annotation>
            <xs:documentation>A verifiable presentation of a set of Europass credentials.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="vp:VerifiablePresentationType">
                <xs:sequence>
                    <xs:element ref="europassCredential" minOccurs="0" maxOccurs="unbounded"/>
                    <xs:element ref="verificationCheck" minOccurs="0" maxOccurs="unbounded"/>
                    <xs:element ref="proof" minOccurs="0" maxOccurs="unbounded"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="VerifiableCredentialType">
        <xs:annotation>
            <xs:documentation>Restricted Verifiable Credential Type</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element ref="dc:issued">
                <xs:annotation>
                    <xs:documentation>The issued date. The value represents the date and time the credential was issued. Note that this date represents the earliest date when the information associated with the credentialSubject property became valid. This is not the date of generation of the credential.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="cred:issuanceDate">
                <xs:annotation>
                    <xs:documentation>The issuance date.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="cred:validFrom">
                <xs:annotation>
                    <xs:documentation>The valid from date. The value of validFrom property represents the date and time from which the credential is valid.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="cred:expirationDate" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>The expiration date. The value of the validUntil property  represents the date and time the credential ceases to be valid.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="cred:issuer" minOccurs="1">
                <xs:annotation>
                    <xs:documentation>The issuer of the credential.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="cred:credentialSubject" minOccurs="1">
                <xs:annotation>
                    <xs:documentation>a set of objects that contain one or more properties that are each related to a subject of the verifiable credential.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="cred:proof" minOccurs="1">
                <xs:annotation>
                    <xs:documentation>One or more cryptographic proofs that can be used to detect tampering and verify the authorship of a credential or presentation. </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
        <xs:attribute name="id" use="required" type="xs:anyURI">
            <xs:annotation>
                <xs:documentation>A portable identifier of the credential.
                    (i.e. a urn in the uuid namespace, urn:uuid:f81d4fae-7dec-11d0-a765-00a0c91e6bf6)</xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>

    <xs:complexType name="EuropassCredentialType">
        <xs:annotation>
            <xs:documentation>A set of claims made by an issuer in Europe, using the Europass Standards. A Europass credential is a set of one or more claims which may be used to demonstrate that the owner has certain skills or has achieved certain learning outcomes through formal, non-formal or informal learning.</xs:documentation>
        </xs:annotation>

        <xs:complexContent>
            <xs:extension base="VerifiableCredentialType">
                <xs:sequence>
                    <xs:element name="type" type="CredentialTypesType">
                        <xs:annotation>
                            <xs:documentation>The type of credential. A concept indicating the type of credential.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element ref="adms:identifier" minOccurs="0" maxOccurs="unbounded">
                        <xs:annotation>
                            <xs:documentation>An alternative identifier of the credential typically assigned to the credential by the issuing organisation.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element ref="dc:title" maxOccurs="unbounded">
                        <xs:annotation>
                            <xs:documentation>The full official title of the issued credential (maximum 50 characters).</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element ref="description" minOccurs="0" maxOccurs="unbounded">
                        <xs:annotation>
                            <xs:documentation>A summary of the claim or group of claims being made about a person (maximum 140 words).</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <!--  TODO finalize controlled list for this -->
                    <xs:element ref="primaryLanguage">
                        <xs:annotation>
                            <xs:documentation>The primary language of the credential (only one language can be applied).</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <!--  TODO finalize controlled list for this -->
                    <xs:element ref="dc:language" minOccurs="1" maxOccurs="29">
                        <xs:annotation>
                            <xs:documentation>The list of minimum 1 to maximum 29 languages the credential content is available in. This list must include the Primary language as well.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element ref="displayParameter" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>The display details of the credential.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element ref="attachment" minOccurs="0" maxOccurs="unbounded">
                        <xs:annotation>
                            <xs:documentation>Any digital document (PDF, JPEG or PNG format) that an issuer has attached to the Europass document.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element ref="hasPart" minOccurs="0" maxOccurs="unbounded">
                        <xs:annotation>
                            <xs:documentation>A credential embedded within the credential. Smaller sub-credentials (micro-credentials), that make up this larger credential when combined. It MUST refer to an existing 'europassCredential'.</xs:documentation>
                        </xs:annotation>
                    </xs:element>

                    <xs:element ref="learningOpportunityReferences" minOccurs="0"/>
                    <xs:element ref="learningAchievementSpecificationReferences" minOccurs="0"/>
                    <xs:element ref="learningOutcomeReferences" minOccurs="0"/>
                    <xs:element ref="learningActivitySpecificationReferences" minOccurs="0"/>
                    <xs:element ref="learningAssessmentSpecificationReferences" minOccurs="0"/>
                    <xs:element ref="learningEntitlementSpecificationReferences" minOccurs="0"/>
                    <xs:element ref="accreditationReferences" minOccurs="0"/>
                    <xs:element ref="agentReferences" minOccurs="0"/>
                    <xs:element ref="learningAchievementReferences" minOccurs="0"/>
                    <xs:element ref="learningActivityReferences" minOccurs="0"/>
                    <xs:element ref="learningAssessmentReferences" minOccurs="0"/>
                    <xs:element ref="learningEntitlementReferences" minOccurs="0"/>
                    <xs:element ref="awardingProcessReferences" minOccurs="0"/>
                    <xs:element ref="gradingSchemeReferences" minOccurs="0"/>
                    <xs:element ref="locationReferences" minOccurs="0"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    
    <xs:complexType name="VerificationCheckType">
        <xs:sequence>
            <xs:element name="type" type="VerificationTypesType">
                <xs:annotation>
                    <xs:documentation>The type of verification check.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="subject" type="EuropassCredentialType">
                <xs:annotation>
                    <xs:documentation>The credential subject of this verification check.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="status" type="VerificationStatussesType">
                <xs:annotation>
                    <xs:documentation>The result.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="description" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>A free text description of the check and the result.</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
        <xs:attribute name="id" use="required" type="xs:anyURI"/>
    </xs:complexType>

    <xs:complexType name="ProofType">
        <xs:annotation>
            <xs:documentation>The cryptographic proofs that can be used to detect tampering and verify the authorship of a credential or presentation.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element ref="ds:Signature" minOccurs="0"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="ClaimType">
        <xs:annotation>
            <xs:documentation>A claim made by an issuer.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element ref="adms:identifier" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element ref="dc:title" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element ref="description" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element ref="additionalNote" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element ref="supplementaryDocument" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="hasPart" type="ClaimType" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="specifiedBy" type="SpecificationType" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
        <xs:attribute name="id" use="required" type="xs:anyURI"/>
    </xs:complexType>
    
    <xs:complexType name="RestrictedClaimType">
        <xs:annotation>
            <xs:documentation>A claim made by an issuer.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element ref="adms:identifier" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element ref="dc:title" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element ref="description" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element ref="additionalNote" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element ref="supplementaryDocument" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
        <xs:attribute name="id" use="required" type="xs:anyURI"/>
    </xs:complexType>

    <xs:complexType name="LearningActivityType">
        <xs:annotation>
            <xs:documentation>Any process which leads to the acquisition of knowledge, skills or responsibility and autonomy.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="RestrictedClaimType">
                <xs:sequence>
                    <xs:element ref="workload" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>The actual workload in number of hours the learner has spent engaged in the activity. This would include the number of hours in class, in group work, in practicals, as well as hours engaged in self-motivated study.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element ref="location" minOccurs="0" maxOccurs="unbounded">
                        <xs:annotation>
                            <xs:documentation>The location where the activity took place. It MUST refer to an existing 'location'-record in the'locationReferences'-section.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element ref="directedBy" minOccurs="0" maxOccurs="unbounded">
                        <xs:annotation>
                            <xs:documentation>The organisation, or part of an organisation such as department, faculty, which directed the learning activity. It MUST refer to an existing 'organization'-record in the 'agentReferences'-section.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element ref="specifiedBy" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>The specification of this learning activity. It MUST refer to an existing 'learningActivitySpecification'-record in the'learningActivitySpecificationReferences'-section.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element ref="learningOpportunity" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>The used or taken opportunity to do this learning activity. It MUST refer to an existing 'learningOpportunity'-record in the'learningOpportunityReferences'-section.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element ref="temporal" minOccurs="0" maxOccurs="1"/>
                    <xs:element ref="influenced" minOccurs="0" maxOccurs="unbounded">
                        <xs:annotation>
                            <xs:documentation>Performing this activity contributed to the acquisition of these related learning achievements. It MUST refer to an existing 'learningAchievement'-record in the 'learningAchievementReferences'-section.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element ref="hasPart" minOccurs="0" maxOccurs="unbounded">
                        <xs:annotation>
                            <xs:documentation>Smaller units of activity, which when combined make up this activity. It MUST refer to an existing 'LearningActivity'-record in the'learningActivityReferences'-section.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="LearningAssessmentType">
        <xs:annotation>
            <xs:documentation>The result of a process establishing the extent to which a learner has attained particular knowledge, skills and competences against criteria such as learning outcomes or standards of competence.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="RestrictedClaimType">
                <xs:sequence>
                    <xs:element ref="grade">
                        <xs:annotation>
                            <xs:documentation>A resulting grade of the assessment.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element ref="shortenedGrading" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>Indicator of how well the student was graded when compared to other students.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element ref="resultDistribution" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>Describes a histogram of results achieved by all the students of a particular learning assessment.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element ref="dc:issued" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>Date the grade was awarded.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="idVerification" type="MethodsOfSupervisionAndVerificationType" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>Method of assessment supervision and id verification.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element ref="assessedBy" minOccurs="0" maxOccurs="unbounded">
                        <xs:annotation>
                            <xs:documentation>The competent body that awarded the grade. It MUST refer to an existing 'organization'-record in the 'agentReferences'-section.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element ref="assesses" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>The learning achievement (and related learning outcomes) which were acquired by successfully passing the assessment. It MUST refer to an existing 'learningAchievement'-record in the 'learningAchievementReferences'-section.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element ref="specifiedBy" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>The specification of this assessment. It MUST refer to an existing 'LearningAssessmentSpecification'-record in the'learningAssessmentSpecificationReferences'-section.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element ref="hasPart" minOccurs="0" maxOccurs="unbounded">
                        <xs:annotation>
                            <xs:documentation>Smaller assessments, which when combined make up and can influence this assessment. It MUST refer to an existing 'LearningAssessment'-record in the'learningAssessmentReferences'-section.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="LearningAchievementType">
        <xs:annotation>
            <xs:documentation>The acquisition of knowledge, skills or responsibility and autonomy. A recognised and/or awarded set of learning outcomes of an individual.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="RestrictedClaimType">
                <xs:sequence>
                    <xs:element name="wasDerivedFrom" type="IdReferenceType" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>An assessment which proves the acquisition of the learning outcomes which make up the achievement. It MUST refer to an existing 'LearningAssessment'-record in the'learningAssessmentReferences'-section.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element ref="wasInfluencedBy" minOccurs="0" maxOccurs="unbounded">
                        <xs:annotation>
                            <xs:documentation>Activities which contributed to the acquisition of the learning outcomes which make up the achievement. It MUST refer to an existing 'LearningActivity'-record in the'learningActivityReferences'-section.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element ref="wasAwardedBy" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>The awarding details of this achievement. It MUST refer to an existing 'awardingProcess'-record in the'awardingProcessReferences'-section.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element ref="entitlesTo" minOccurs="0" maxOccurs="unbounded">
                        <xs:annotation>
                            <xs:documentation>Entitlements the owner has received as a result of this achievement. It MUST refer to an existing 'LearningEntitlement'-record in the'learningActivityReferences'-section.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element ref="hasPart" minOccurs="0" maxOccurs="unbounded">
                        <xs:annotation>
                            <xs:documentation>Smaller units of achievement, which when combined make up this achievement. It MUST refer to an existing 'learningAchievement'-record in the 'learningAchievementReferences'-section.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element ref="specifiedBy" minOccurs="0" maxOccurs="unbounded">
                        <xs:annotation>
                            <xs:documentation>What has been learned. It MUST refer to an existing 'learningAchievementSpecification' record in the 'learningAchievementSpecificationReferences-section.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element ref="learningOpportunity" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>The learning opportunity that was taken to obtain the awarded LearningSpecification. It MUST refer to an existing 'learningOpportunity'-record in the'learningOpportunityReferences'-section.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="LearningEntitlementType">
        <xs:annotation>
            <xs:documentation>A right, e.g. to practice a profession, take advantage of a learning opportunity or join an organisation, as a result of the acquisition of knowledge, skills, responsibility and/or autonomy.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="RestrictedClaimType">
                <xs:sequence>
                    <xs:element ref="temporal" minOccurs="0"/>
                    <xs:element ref="specifiedBy" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>A learning achievement which gave rise to the entitlement. It MUST refer to an existing 'learningEntitlementSpecification'-record in the 'learningEntitlementSpecificationReferences'-section.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="wasDerivedFrom" type="IdReferenceType" minOccurs="0"
                        maxOccurs="unbounded">
                        <xs:annotation>
                            <xs:documentation>The learning achievement (and related learning outcomes) which gave rise to this entitlement. It MUST refer to an existing 'learningAchievement'-record in the 'learningAchievementReferences'-section.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element ref="hasPart" minOccurs="0" maxOccurs="unbounded">
                        <xs:annotation>
                            <xs:documentation>Smaller learning entitlements, which when combined make up this entitlement. It MUST refer to an existing 'LearningEntitlement'-record in the'learningActivityReferences'-section.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>


    <xs:complexType name="SpecificationType">
        <xs:annotation>
            <xs:documentation>A specification</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element ref="adms:identifier" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element ref="dc:type" minOccurs="0"/>
            <xs:element ref="dc:title" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element ref="skos:altLabel" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element ref="description" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element ref="additionalNote" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element ref="homepage" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element ref="supplementaryDocument" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="hasPart" type="SpecificationType" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="specializationOf" type="SpecificationType" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
        <xs:attribute name="id" use="required" type="xs:anyURI"/>
    </xs:complexType>
    
    <xs:complexType name="RestrictedSpecificationType">
        <xs:annotation>
            <xs:documentation>A specification</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element ref="adms:identifier" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element ref="dc:title" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element ref="skos:altLabel" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element ref="description" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element ref="additionalNote" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element ref="homepage" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element ref="supplementaryDocument" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
        <xs:attribute name="id" use="required" type="xs:anyURI"/>
    </xs:complexType>
    
    <xs:complexType name="LearningAchievementSpecificationType">
        <xs:annotation>
            <xs:documentation>A description of what a person may learn using the opportunity, expressed as learning outcomes. A specification of learning.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="RestrictedSpecificationType">
                <xs:sequence>
                    <xs:element name="type" type="LearningOpportunityTypesType" minOccurs="0" maxOccurs="unbounded">
                        <xs:annotation>
                            <xs:documentation>The type of learning opportunity.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="ISCEDFCode" type="ISCED-FCodeType" minOccurs="0" maxOccurs="unbounded">
                        <xs:annotation>
                            <xs:documentation>Thematic Area according to the ISCED-F 2013 Classification</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element ref="educationSubject" minOccurs="0" maxOccurs="unbounded">
                        <xs:annotation>
                            <xs:documentation>An associated field of education from another semantic framework than the ISCED classification.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element ref="volumeOfLearning" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>The estimated number of hours the learner is expected to spend engaged in learning to earn the award. This would include the notional number of hours in class, in group work, in practicals, as well as hours engaged in self-motivated study.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element ref="ECTSCreditPoints" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>The credit points assigned to the learning specification, following the ECTS credit system.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element ref="creditPoints" minOccurs="0" maxOccurs="unbounded">
                        <xs:annotation>
                            <xs:documentation>The credit points assigned to the learning specification, following an alternative educational credit system.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element ref="educationLevel" minOccurs="0" maxOccurs="unbounded">
                        <xs:annotation>
                            <xs:documentation>An associated level of education within a semantic framework describing education levels.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element ref="language" minOccurs="0" maxOccurs="unbounded">
                        <xs:annotation>
                            <xs:documentation>The instruction and/or assessment language(s) used.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element ref="mode" minOccurs="0" maxOccurs="unbounded">
                        <xs:annotation>
                            <xs:documentation>The mode of learning and or assessment.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element ref="learningSetting" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>The type of learning setting (formal, non-formal).</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element ref="maximumDuration" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>The maximum duration (in months) that a person may use to complete the learning opportunity.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element ref="targetGroup" minOccurs="0" maxOccurs="unbounded">
                        <xs:annotation>
                            <xs:documentation>A specific target group or category for which this specification is designed.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element ref="entryRequirements" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>Specific entry requirements or prerequisites of individuals for which this specification is designed to start this learning opportunity.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element ref="learningOutcome" minOccurs="0" maxOccurs="unbounded">
                        <xs:annotation>
                            <xs:documentation>An individual (expected) learning outcome of the learning specification. It MUST refer to an existing 'LearningOutcome'-record in the 'learningOutcomeReferences'-section.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element ref="learningActivitySpecification" minOccurs="0"
                        maxOccurs="unbounded">
                        <xs:annotation>
                            <xs:documentation>Activities which a person can perform to acquire the expected learning outcomes. It MUST refer to an existing 'LearningActivitySpecification'-record in the 'learningActivitySpecificationReferences'-section.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element ref="learningAssessmentSpecification" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>Assessments a person can undergo to prove the acquisition of the learning outcomes. It MUST refer to an existing 'LearningAssessmentSpecification'-record in the 'learningAssessmentSpecificationReferences'-section.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element ref="learningEntitlementSpecification" minOccurs="0"
                        maxOccurs="unbounded">
                        <xs:annotation>
                            <xs:documentation>Rights (such as which the person may acquire as a result of acquiring the learning outcomes). It MUST refer to an existing 'LearningEntitlementSpecification'-record in the 'learningEntitlementSpecificationReferences'-section.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element ref="hasPart" minOccurs="0" maxOccurs="unbounded">
                        <xs:annotation>
                            <xs:documentation>A learning specification can be composed of other learning specifications which when combined make up this learning specification. It MUST refer to an existing 'learningAchievementSpecification'record in the 'learningAchievementSpecificationReferences-section.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element ref="specializationOf" minOccurs="0" maxOccurs="unbounded">
                        <xs:annotation>
                            <xs:documentation>A learning specification (e.g. a standard) of which this specification is a specialisation. It MUST refer to an existing 'learningAchievementSpecification'record in the 'learningAchievementSpecificationReferences-section.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element ref="awardingOpportunity" minOccurs="0" maxOccurs="unbounded">
                        <xs:annotation>
                            <xs:documentation>Refers to an activity related to the awarding of the learning specification, such as the country or region where the qualification is awarded, the awarding body and optionally the awarding period now or in the past.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="QualificationType">
        <xs:annotation>
            <xs:documentation>a specification of an assessment and validation process which is obtained when a competent authority determines that an individual has achieved learning outcomes to given standards.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="LearningAchievementSpecificationType">
                <xs:sequence>
                    <xs:element ref="isPartialQualification" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>Indicates whether a qualification is a full qualification or part of another qualification.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element ref="EQFLevel" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>The qualification level as specified by the European Qualification Framework.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element ref="NQFLevel" minOccurs="0" maxOccurs="unbounded">
                        <xs:annotation>
                            <xs:documentation>The qualification level as specified by a National Qualification Framework.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element ref="accreditation" minOccurs="0" maxOccurs="unbounded">
                        <xs:annotation>
                            <xs:documentation>The accreditation of a qualification. It MUST refer to an existing 'accreditation'-record in the 'accreditationReferences'-section.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="LearningOutcomeType">
        <xs:annotation>
            <xs:documentation>A statement regarding what a learner knows, understands and is able to do on completion of a learning process, which are defined in terms of knowledge, skills and responsibility and autonomy.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element ref="adms:identifier" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>An alternative identifier of the learning outcome.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="dc:title" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>A legible, descriptive name for the learning outcome.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="type" type="LearningOutcomeTypesType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>The learning outcome type.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="reusabilityLevel" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>The reusability level.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="relatedSkill" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>A link to a related skill or the level of a related skill on a skill framework (except ESCO).</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="relatedESCOSkill" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>A link to an ESCO Skill.</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
        <xs:attribute name="id" use="required" type="xs:anyURI">
            <xs:annotation>
                <xs:documentation>A portable and unique identifier of the learning outcome.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>

    <xs:complexType name="LearningActivitySpecificationType">
        <xs:annotation>
            <xs:documentation>The specification of a process which leads to the acquisition of knowledge, skills or responsibility and autonomy.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="RestrictedSpecificationType">
                <xs:sequence>
                    <xs:element name="type" type="LearningActivityTypesType" minOccurs="0" maxOccurs="unbounded">
                        <xs:annotation>
                            <xs:documentation>The type of activity.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element ref="volumeOfLearning" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>The estimated number of hours the learner is expected to spend engaged in the activity. This would include the notional number of hours in class, in group work, in practicals, as well as hours engaged in self-motivated study.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element ref="language" minOccurs="0" maxOccurs="unbounded">
                        <xs:annotation>
                            <xs:documentation>The instruction language(s) used.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element ref="mode" minOccurs="0" maxOccurs="unbounded">
                        <xs:annotation>
                            <xs:documentation>The mode of learning and or assessment.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element ref="teaches" minOccurs="0" maxOccurs="unbounded">
                        <xs:annotation>
                            <xs:documentation>What may be learned by undertaking the activity described by this learning activity specification. It MUST refer to an existing 'learningAchievementSpecification'record in the 'learningAchievementSpecificationReferences-section.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element ref="hasPart" minOccurs="0" maxOccurs="unbounded">
                        <xs:annotation>
                            <xs:documentation>A learning activity specification can be composed of smaller learning activity specifications, which when combined make up this learning activity specification. It MUST refer to an existing 'learningActivitySpecification'record in the 'learningActivitySpecificationReferences-section.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element ref="specializationOf" minOccurs="0" maxOccurs="unbounded">
                        <xs:annotation>
                            <xs:documentation>A learning specification (e.g. a standard) of which this specification is a specialisation. It MUST refer to an existing 'learningActivitySpecification'record in the 'learningActivitySpecificationReferences-section.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="LearningAssessmentSpecificationType">
        <xs:annotation>
            <xs:documentation>A Learning Assessment Specification is a specification of a process establishing the extent to which a learner has attained particular knowledge, skills and competences against criteria such as learning outcomes or standards of competence.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="RestrictedSpecificationType">
                <xs:sequence>
                    <xs:element name="type" type="LearningAssessmentTypesType" minOccurs="0" maxOccurs="unbounded">
                        <xs:annotation>
                            <xs:documentation>The type of assessment.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element ref="language" minOccurs="0" maxOccurs="unbounded">
                        <xs:annotation>
                            <xs:documentation>The language(s) of assessment used.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element ref="mode" minOccurs="0" maxOccurs="unbounded">
                        <xs:annotation>
                            <xs:documentation>The mode of learning and or assessment.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element ref="gradingScheme" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation>A description of the specification of which learning outcomes are or have been proven. It MUST refer to an existing 'gradingScheme" record in the 'gradingSchemeReferences-section.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element ref="proves" minOccurs="0" maxOccurs="unbounded">
                        <xs:annotation>
                            <xs:documentation>The learning achievement (and related learning outcomes) this assessment is designed to test. It MUST refer to an existing 'learningAchievementSpecification' record in the 'learningAchievementSpecificationReferences-section.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element ref="hasPart" minOccurs="0" maxOccurs="unbounded">
                        <xs:annotation>
                            <xs:documentation>A learning activity specification can be composed of smaller learning activity specifications, which when combined make up this learning activity specification. It MUST refer to an existing 'learningAssessmentSpecification'record in the 'learningAssessmentSpecificationReferences-section.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element ref="specializationOf" minOccurs="0" maxOccurs="unbounded">
                        <xs:annotation>
                            <xs:documentation>A learning specification (e.g. a standard) of which this specification is a specialisation. It MUST refer to an existing 'learningAssessmentSpecification'record in the 'learningAssessmentSpecificationReferences-section.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="LearningEntitlementSpecificationType">
        <xs:annotation>
            <xs:documentation>The specification of a right a person has access to, typically as a result of a learning achievement. It may take the form of the right to be a member of an organisation, to follow a certain learning opportunity specification, or to follow a certain career.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="RestrictedSpecificationType">
                <xs:sequence>
                    <xs:element name="type" type="LearningEntitlementTypesType">
                        <xs:annotation>
                            <xs:documentation>A credential-holder may be entitled to membership of an organisation or professional association; to access a learning opportunity; or to perform a specific employment</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="status" type="LearningEntitlementStatusesType">
                        <xs:annotation>
                            <xs:documentation>The status of the entitlement: an entitlement may be prospective, i.e. awarding the right to apply for the entitlement; or actual, i.e. granting the entitlement.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element ref="limitOrganization" minOccurs="0" maxOccurs="unbounded">
                        <xs:annotation>
                            <xs:documentation>The organisation which acknowledges the entitlement (i.e. the organisation offering the learning opportunity, membership or employment opportunity). It MUST refer to an existing 'organization' record in the 'agentReferences'-section.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element ref="limitJurisdiction" minOccurs="0" maxOccurs="unbounded">
                        <xs:annotation>
                            <xs:documentation>The jurisdiction for which the entitlement is valid (the region or country).</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element ref="limitOccupation" minOccurs="0" maxOccurs="unbounded">
                        <xs:annotation>
                            <xs:documentation>An ESCO Occupation or Occupational class which the individual may access through the entitlement.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element ref="limitNationalOccupation" minOccurs="0" maxOccurs="unbounded">
                        <xs:annotation>
                            <xs:documentation>An Occupation or Occupational Category</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element ref="mayResultFrom" minOccurs="0" maxOccurs="unbounded">
                        <xs:annotation>
                            <xs:documentation>A LearningAchievementSpecification that may give rise to an entitlement with this specification. It MUST refer to an existing 'learningAchievementSpecification' record in the 'learningAchievementSpecificationReferences-section.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element ref="hasPart" minOccurs="0" maxOccurs="unbounded">
                        <xs:annotation>
                            <xs:documentation>A learning activity specification can be composed of smaller learning activity specifications, which when combined make up this learning activity specification. It MUST refer to an existing 'learningEntitlementSpecification'record in the 'learningEntitlementSpecificationReferences-section.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element ref="specializationOf" minOccurs="0" maxOccurs="unbounded">
                        <xs:annotation>
                            <xs:documentation>A learning specification (e.g. a standard) of which this specification is a specialisation. It MUST refer to an existing 'learningEntitlementSpecification'record in the 'learningEntitlementSpecificationReferences-section.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="ShortenedGradingType">
        <xs:annotation>
            <xs:documentation>Indicator of how well the student was graded when compared to other students.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element ref="percentageLower">
                <xs:annotation>
                    <xs:documentation>The percentage of students of the same course who got a lower grade.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="percentageEqual">
                <xs:annotation>
                    <xs:documentation>The percentage of students of the same course who got exactly the same grade.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="percentageHigher">
                <xs:annotation>
                    <xs:documentation>The percentage of students of the same course who got a higher grade.</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="ResultDistributionType">
        <xs:annotation>
            <xs:documentation>Describes a histogram of results achieved by all the students of this course instance.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element ref="resultCategory" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>Describes a single range within the histogram.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="description" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>Free text description of the histogram.</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="ResultCategoryType">
        <xs:annotation>
            <xs:documentation>Description of a single score or score range within a histogram of results.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element ref="label">
                <xs:annotation>
                    <xs:documentation>The label of the histogram score or score range. Should correspond to the grading scheme which have been used. E.g. 'C', or '20-30'.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="score" minOccurs="0"/>
            <xs:element ref="minScore" minOccurs="0"/>
            <xs:element ref="maxScore" minOccurs="0"/>
            <xs:element ref="count"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="GradingSchemeType">
        <xs:annotation>
            <xs:documentation>A set of criteria that measures varying levels of achievement.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element ref="adms:identifier" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>An alternative identifier of the Grading Scheme assigned to it by the organisation administering the scheme.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="dc:title" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>The title of the grading scheme.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="description" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>A free text describing the grading scheme.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="supplementaryDocument" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>A public web document containing additional documentation about the grading system.</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
        <xs:attribute name="id" use="required" type="xs:anyURI">
            <xs:annotation>
                <xs:documentation>A portable and unique identifier of the Grading Scheme.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>

    <xs:complexType name="AwardingProcessType">
        <xs:annotation>
            <xs:documentation>The process of an organisation awarding Learning Achievement to person based on a Learning Specification (e.g. a qualification). It is used to specify the organisation that awarded the LearningSpecification to the individual, the country or region where the LearningSpecification was awarded, and optionally the date of awarding.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element ref="adms:identifier" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>An alternative identifier of the awarding process.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="description" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>A description of the awarding process to the individual.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="additionalNote" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>An additional free text note (e.g. a comment, a remark, etc.)</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="used" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>The assessment that provided the basis for this awarding. It MUST refer to an existing 'LearningAssessment'-record in the'learningAssessmentReferences'-section.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="learningAchievement" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>The resulting learning achievement. It MUST refer to an existing 'LearningAchievement'-record in the'learningAchievementReferences'-section of this document.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="awardingBody" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>The awarding body that awarded the Achievement to the individual. Only in cases of co-awarding/co-graduation, where a qualification award is issued to an individual by two or more organisations the cardinality is greater than 1. It MUST refer to an existing 'organization'-record in the 'agentReferences'-section.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="location" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>The location where the awarding activity took place (country/region where the qualification was awarded). It MUST refer to an existing 'location'-record in the'locationReferences'-section.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="dc:issued" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>The date when the LearningSpecification was awarded. If not specified it is undefined ("not known").</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
        <xs:attribute name="id" use="required" type="xs:anyURI">
            <xs:annotation>
                <xs:documentation>A portable and Unique Identifier of the Awarding Process.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>

    <xs:complexType name="AwardingOpportunityType">
        <xs:annotation>
            <xs:documentation>An awarding activity represents an activity related to the awarding of a LearningSpecification. It is used to specify the country or region where the LearningSpecification is awarded, the awarding body and optionally the awarding period now or in the past.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element ref="adms:identifier" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>An alternative identifier of the awarding opportunity.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="awardingBody" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>The awarding body related to this awarding activity (i.e the organisation that issues the qualification) Only in cases of co-awarding/co-graduation, where a qualification is issued to an individual by two or more organisations the cardinality is greater than 1. It MUST refer to an existing 'organization'-record in the 'agentReferences'-section.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="location" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Location where the awarding activity takes place (country/region where the qualification is awarded). It MUST refer to an existing 'location'-record in the'locationReferences'-section.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="temporal" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Temporal coverage of the awardingOpportunity. It includes The date since when the awarding activities take place and the date until when the awarding activities take/took place.</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
        <xs:attribute name="id" use="required" type="xs:anyURI">
            <xs:annotation>
                <xs:documentation>The portable and unique identifier of the awarding opportunity record.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>

    <xs:complexType name="LearningOpportunityType">
        <xs:annotation>
            <xs:documentation>An opportunity to realise a given set of learning outcomes via a learning activity and/or assessment.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element ref="adms:identifier" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>An alternative identifier assigned to the learning opportunity by the organisation offering the opportunity.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="dc:title" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>The title of the learning opportunity.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="description" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>A free text description (summary) of the learning opportunity.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="additionalNote" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>An additional free text note about the learning opportunity.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="homepage" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>The homepage (a public web document) of the learning opportunity.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="supplementaryDocument" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>A public web document containing additional documentation about the learning opportunity.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="temporal" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Temporal coverage of the learning opportunity. It includes the start date from which a person may follow the learning opportunity and the final date a person may start to follow the learning opportunity.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="duration" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>The nominal duration for which the learning opportunity will continue to run. Note, this may be after the end-date, since admissions may be closed but the learning opportunity may still be ongoing.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="learningSchedule" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>The learning schedule.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="scheduleInformation" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Detailed information about the timetable or schedule. This may include weekly schedules (e.g. "Every Monday, 4pm", but may also include the overall schedule for the course, (e.g. October: lectures, November: group-work, December: break, January: assessment).</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="admissionProcedure" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Specific information on how to apply for the course.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="priceDetails" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>Information about the pricing of the course, including fees and scholarships/subsidies available</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="providedBy" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>The organisation providing or directing the learning opportunity. In the case of, e.g. joint qualifications, there may be several organisations directing the learning opportunity. It MUST refer to an existing 'organization'-record in the 'agentReferences'-section.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="providedAt" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>The location where the learning opportunity will take place. This may also include a virtual space. It MUST refer to an existing 'location'-record in the'locationReferences'-section.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="learningAchievementSpecification" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>The learning achievement specification, including the curricula, of this learning opportunity. It MUST refer to an existing 'learningAchievementSpecification' record in the 'learningAchievementSpecificationReferences-section.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="hasPart" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>A learning opportunity can be composed of other learning opportunities, which when combined make up this larger learning opportunity. It MUST refer to an existing 'learningOpportunity' record in the 'learningOpportunityReferences'-section.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="bannerImage" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>An image which is displayed alongside the learning opportunity.</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
        <xs:attribute name="id" use="required" type="xs:anyURI">
            <xs:annotation>
                <xs:documentation>The unique and portable identifier of the Learning Opportunity.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>

    <xs:complexType name="PriceDetails">
        <xs:annotation>
            <xs:documentation>The price details. The details about a price or price category.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element ref="adms:identifier" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>The identifier of the price category, allocated by the organisation charging the fee.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="amount" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>The full price.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="skos:prefLabel" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>The name of the price/price category.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="description" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>A free text describing the price.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="additionalNote" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>An additional free text note about the price.</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
        <xs:attribute name="id" use="required" type="xs:anyURI">
            <xs:annotation>
                <xs:documentation>The portable and unique identifier of the Price Details.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>

    <xs:complexType name="AmountType">
        <xs:annotation>
            <xs:documentation>An amount.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="value" type="xs:decimal">
                <xs:annotation>
                    <xs:documentation>A numeric value.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="unit">
                <xs:annotation>
                    <xs:documentation>A code indicating the currency in which the amount is indicated/expressed.</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="AccreditationType">
        <xs:annotation>
            <xs:documentation>The quality assurance or licensing of an organisation or a qualification. An accreditation instance can be used to specify information about: - the quality assurance and/or licensing of an organisation. - the quality assurance and/or licensing of an organisation with respect to a specific qualification.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element ref="adms:identifier" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>An alternative Identifier of the Accreditation, as assigned to it by the accrediting agent.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="type" type="AccreditationTypesType">
                <xs:annotation>
                    <xs:documentation>The type of accreditation.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="dc:title" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>A title of the accreditation.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="description" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>A free text description of the accreditation.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="decision" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>The Quality Decision issued by the Quality Assuring Authority.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="report" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>A publicly accessible report of the quality assurance decision.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="organization">
                <xs:annotation>
                    <xs:documentation>The organisation whose activities are being accredited. It MUST refer to an existing 'organization'-record in the 'agentReferences'-section.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="limitQualification" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>The qualification that was accredited. It MUST refer to an existing 'Qualification'-record in the 'learningSpecificationReferences'-section.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="limitField" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>The field of education for which the accreditation is valid.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="limitEQFLevel" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>The european qualification level for which the accreditation is valid.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="limitJurisdiction" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>The jurisdiction for which the accreditation is valid.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="accreditingAgent">
                <xs:annotation>
                    <xs:documentation>The Quality Assuring Authority. (i.e assurer). It MUST refer to an existing 'organization'-record in the 'agentReferences'-section.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="temporal" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Temporal coverage of the accreditation. It includes the date when the accreditation was formally approved/issued and the date when the accreditation expires or was expired.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="reviewDate" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>The date when the accreditation has to be re-viewed.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="additionalNote" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>An additional free text note about the accreditation.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="homepage" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>The homepage of the accreditation.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="landingPage" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>The landing page of the accreditation.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="supplementaryDocument" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>A public web document containing additional documentation describing the Accreditation Procedures and Standards.</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
        <xs:attribute name="id" use="required" type="xs:anyURI">
            <xs:annotation>
                <xs:documentation>A portable and unique identifier of the accreditation.</xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>

    <xs:complexType name="DisplayParameterType">
        <xs:annotation>
            <xs:documentation/>
        </xs:annotation>
        <xs:sequence>
            <xs:element ref="summaryDisplay" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>The code indicating how to display the summary view of the credential.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="background" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>The background image of the credential.</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="InteractiveWebResourceType">
        <xs:annotation>
            <xs:documentation>A web resource that can be used to communicate.</xs:documentation>
        </xs:annotation>
        <xs:simpleContent>
            <xs:extension base="xs:anyURI"/>
        </xs:simpleContent>
    </xs:complexType>

    <xs:complexType name="PhoneType">
        <xs:annotation>
            <xs:documentation>A phone.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element ref="phoneNumber" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>The full phone number as a string.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element ref="countryDialing" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>The country dialing code for a contact number.</xs:documentation>
                </xs:annotation>
