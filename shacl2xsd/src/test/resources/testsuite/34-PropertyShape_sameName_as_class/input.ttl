# The NodeShape
edci-c:MyBeautifulEuropassCredentialShape 
  a sh:NodeShape; 
  sh:targetClass elm:EuropassCredential; 
  sh:property edci-c:EuropassCredential-elm_mediaObject;
  .

edci-c:EuropassCredential-elm_mediaObject
  a sh:PropertyShape; 
  # the name of the property is identical to the name of the class
  sh:path elm:mediaObject;
  sh:class elm:MediaObject;
  .  

edci-c:MediaObjectShape 
  a sh:NodeShape; 
  sh:targetClass elm:MediaObject; 
  .